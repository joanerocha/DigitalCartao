<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="joane.digitalcartao">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".telas.inicio.tela_inicio" />
        <activity android:name=".telas.cadastro.cadastro_medico" />
        <activity android:name=".telas.cadastro.cadastro_paciente" />
        <activity android:name=".telas.busca.buscarPaciente" />
        <activity android:name=".telas.inicio.tela_medico" />
        <activity
            android:name=".CartaoVacina"
            android:screenOrientation="landscape"></activity>

        <meta-data
            android:name="DATABASE"
            android:value="sug.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="joane.digitalcartao" />

        <activity android:name=".TelaLogin"></activity>
    </application>

</manifest>